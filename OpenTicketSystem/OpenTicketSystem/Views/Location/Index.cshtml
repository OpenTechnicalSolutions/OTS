
@{
    ViewData["Title"] = "Location Management";
}
<script src="~/lib/locationCall.js" type="text/javascript"></script>
<h2>Location Management</h2>

<button class="btn btn-primary" id="departments">Departments</button>
<button class="btn btn-primary" id="buildings">Buildings</button>
@if(ViewBag.LocationIndex == 0)
{
    <script>
        queryData('Department')
    </script>
} else if(ViewBag.LocationIndex == 1)
{
    <script>
        queryData('Building');
    </script>
}
<div id="partialView">

</div>