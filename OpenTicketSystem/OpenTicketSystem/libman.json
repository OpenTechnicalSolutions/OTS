{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "jquery@3.3.1",
      "destination": "wwwroot/lib/jquery/"
    },
    {
      "library": "bootstrap-rtl@3.4.0",
      "destination": "wwwroot/bootstrap-rtl/"
    },
    {
      "library": "popper.js@1.14.4",
      "destination": "wwwroot/bootstrap-rtl/popper.js/"
    },
    {
      "provider": "filesystem",
      "library": "https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css",
      "destination": "wwwroot/lib/bootstrap.min/"
    },
    {
      "provider": "filesystem",
      "library": "https://code.jquery.com/jquery-3.3.1.slim.min.js",
      "destination": "wwwroot/jquery-3.3.1.slim.min/"
    },
    {
      "provider": "filesystem",
      "library": "https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.3/umd/popper.min.js",
      "destination": "wwwroot/popper.min/"
    },
    {
      "provider": "filesystem",
      "library": "https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/js/bootstrap.min.js",
      "destination": "wwwroot/bootstrap.min/"
    }
  ]
}